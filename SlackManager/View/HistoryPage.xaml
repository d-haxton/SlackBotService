<UserControl x:Class="SlackManager.View.HistoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SlackManager.View"
      mc:Ignorable="d" 
      DataContext="{Binding HistoryDataContext, Source={StaticResource Locator}}"
      d:DesignHeight="300" d:DesignWidth="500">

    <Grid>
        <DataGrid ItemsSource="{Binding HistoryItems}"/>
    </Grid>
</UserControl>
