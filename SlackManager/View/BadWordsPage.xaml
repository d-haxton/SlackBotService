<UserControl x:Class="SlackManager.View.BadWordsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SlackManager.View"
             mc:Ignorable="d" 
                   DataContext="{Binding BadWordsDataContext, Source={StaticResource Locator}}"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
          <DataGrid ItemsSource="{Binding BadWordItems}"/>
    </Grid>
</UserControl>
